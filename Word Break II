s = "catsanddog"
wordDict = ["cat","cats","and","sand","dog"]


dp = {}
def wordBreak(s):
    if s in dp:
        return dp[s]
    result = []
    for w in wordDict:
        if s[:len(w)] == w:
            if len(w) == len(s):
                result.append(w)
            else:
                tmp = wordBreak(s[len(w):])
                for t in tmp:
                    result.append(w + " " + t)
                    
    dp[s] = result
    return result
        
print(wordBreak(s))
